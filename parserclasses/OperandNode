package parserclasses;
import java.util.ArrayList;
import package provided JottTree;
import package provided Token;
import provided.TokenType;


public interface OperandNode implements JottTree{

    static Node parseOperand(ArrayList<Token> tokens){
        currToken = tokens.get(0);
        if (currToken.getTokenType() == TokenType.ID_KEYWORD){
            Node id = new IDNode(currToken);
            return id.parseIDNode(currToken);

        }
        else if (currToken.getTokenType() == TokenType.NUMBER){
            //Check if negative 
            //Put flag in number node
            Node number = new NumberNode(currToken);
            return number.parseNumberNode(currtoken);

        }
        else if (currToken.getTokenType() == TokenType.FC_HEADER){
            Node func = new FunctioncallNode(currToken);
            return func.parseFunctionCallNode(currtoken);

        }
        else{
            return "Invalid Node"
        }
    }
}